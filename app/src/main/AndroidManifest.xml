<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.big.simplecash">

    <application
        android:name="com.big.simplecash.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.big.simplecash.material.MaterialActivity" />
        <activity android:name="com.big.simplecash.AddActivity" />
        <activity android:name="com.big.simplecash.SaleActivity" />
        <activity android:name="com.big.simplecash.OrderListActivity" />
        <activity android:name="com.big.simplecash.SettlementActivity" />
    </application>

</manifest>